{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "evaluation/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/evaluation/(\\d+)",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.js"
    },
    {
      "src": "/training",
      "dest": "/api/index.js"
    },
    {
      "src": "/how-to-take-training",
      "dest": "/api/index.js"
    },
    {
      "src": "/coursera-week2-alternative",
      "dest": "/coursera_week2_alternative.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
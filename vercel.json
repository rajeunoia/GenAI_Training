{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/training",
      "dest": "/GenAi_Training.html"
    },
    {
      "src": "/how-to-take-training",
      "dest": "/how_to_take_training.html"
    },
    {
      "src": "/evaluation/(\\d+)",
      "dest": "/evaluation/genai_week$1_evaluation.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "SESSION_SECRET": "@session_secret",
    "NODE_ENV": "production"
  }
}